{
  "name": "lp-generator",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/pinetree/lp-generator.git"
  },
  "config": {
    "folder": "sample.com"
  },
  "scripts": {
    "dev": "webpack --mode development ./src/websites/%npm_package_config_folder%/index.js --output ./dist/main.js",
    "build": "webpack --mode production ./src/websites/%npm_package_config_folder%/index.js --output ./dist/main.js",
    "start": "webpack-dev-server --mode development ./src/websites/%npm_package_config_folder%/index.js --open",
    "get-folder": "echo Current directory: %npm_package_config_folder%."
  },
